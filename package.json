{
  "name": "orbitus-classroom-rpg",
  "private": true,
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "build": "pnpm -r run build",
    "db:generate": "pnpm --filter api run prisma:generate",
    "db:migrate": "pnpm --filter api run prisma:migrate",
    "db:studio": "pnpm --filter api run prisma:studio"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.0"
}
